function bouncer(arr) {
  // Don't show a false ID to this bouncer.
  arr = arr.filter(filterThis);
  return arr;
}
function filterThis(val){
  if (val === false || val ===0 || val === "" || val === undefined ||!val){
    return false;
  }else {
    return true;
  }
}
bouncer([7, "ate", "", null, 9]);
